<?xml version='1.0' encoding='UTF-8' ?>
<odoo>
    <data>
        <record id="action_hr_holidays_dashboard_manager" model="ir.actions.act_window">
            <field name="name">All Time Off (My Employees)</field>
            <field name="res_model">hr.leave</field>
            <field name="view_mode">calendar,tree,form</field>
            <field name="context">{
                'form_view_ref': 'hr_holidays.hr_leave_view_form_manager',
            }</field>
            <field name="domain">[('holiday_type','=','employee'), ('state', '!=', 'refuse')]</field>
            <field name="view_id" />
        </record>
    </data>
</odoo>